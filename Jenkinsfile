pipeline {
    agent any
        stages {
        stage('Clone Repository') {
            steps {
                // Clona el repositorio
                sh 'git clone https://github.com/deManrrique/hola-mundo.py.git'
            }
        }

   stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                
            }
        }
    }
}
